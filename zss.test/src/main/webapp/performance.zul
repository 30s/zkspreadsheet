<vlayout hflex="1" vflex="1">
	<hlayout>
		<button onClick="setValue()">Set Value</button>
	</hlayout>
	<spreadsheet id="ss" src="/performance.xlsx" maxVisibleRows="500" maxVisibleColumns="40"
		width="1024px" height="800px" showContextMenu="true" showSheetbar="true" showToolbar="true" 
		showFormulabar="true"/>
	<zscript><![CDATA[

	public void setValue(){
	    Sheet sheet = ss.getBook().getSheetAt(0);
		for(int i = 0; i < 40; i++) {
			for(int j = 0; j < 10; j++) {
				Range range = Ranges.range(sheet, i, j);
				range.setCellValue(range.getCellData().getDoubleValue() + 5);
			}
		}
		System.out.println(">>> set value");
	}
	]]></zscript>		
</vlayout>