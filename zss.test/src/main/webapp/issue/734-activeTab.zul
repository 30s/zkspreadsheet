<?page title="ZSS Export to Excel File Format" contentType="text/html;charset=UTF-8"?>
<!--
A dot is replaced with colon in a formula causes exported Excel file corruption under German (Polish) locale 
 -->
<window width="100" height="100%"
	apply="zss.issue.B734_ActiveTabComposer">
	<label multiline="true">
	1. Delete 2nd sheet
	2. Press "Export to Excel" button to export it
	3. Open it with Excel 2007; should open without seeing any errors.
	</label>
	<hbox>
		<button id="exportBtn" label="Export to Excel"></button>
	</hbox>
	<spreadsheet id="spreadsheet" showFormulabar="true"
		src="/issue/734-lastSheetActive.xlsx" maxrows="200" maxcolumns="40" showSheetbar="true"
		vflex="1" width="100%">
	</spreadsheet>
</window>
